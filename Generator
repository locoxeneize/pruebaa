import random
import string

n = 12

def generar_contrase単a(n):
    clave = ""
    caracteres = list(string.printable)
    caracteres = caracteres[:-6]

    for i in range(n):
        clave += random.choice(caracteres)
         #print(i)
         #print(s)
         #print(s[0])

    return clave

archivo=open('Contrase単asGeneradas.txt','w')
archivo.close()
archivo=open('Contrase単asGeneradas.txt','a')


for i in range(100):
    generar = generar_contrase単a(12)
    #print(generar)
    archivo.write(generar) 
    archivo.write('\n')    

caracteres = list(string.printable)
caracteres = caracteres[:-6]

